[paths]
/ = /var/lib/hg/*

[web]
style = gitweb
baseurl = https://hg.example.com
staticurl = /static
contact = Contact <contact@hg.example.com>
pygments_style = tango
highlightfiles = size('<100k')
highlightonlymatchfilename = True
allow-archive = bz2 zip gz

[extensions]
highlight =
evolve.serveronly =

[phases]
publish = False
new-commit = draft
